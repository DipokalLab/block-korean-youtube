{
    "manifest_version": 3,
    "name": "Block Korean YouTube",
    "description": "Chrome extension, block Korean videos on YouTube",
    "version": "1.0.0",
    "content_scripts": [{
        "matches": ["https://www.youtube.com/"],
        "js": ["./script.js"],
        "run_at": "document_start"
    }]
}